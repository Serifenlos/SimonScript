{
  "id": "e3f22fe6",
  "name": "SimonScript Panel",
  "version": "2.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "PS",
      "minVersion": "25.4.0"
    }
  ],
  "manifestVersion": 5,
  "requiredPermissions": {
    "localFileSystem": "fullAccess"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "vanilla",
      "minimumSize": {
        "width": 1,
        "height": 1
      },
      "maximumSize": {
        "width": 2000,
        "height": 600
      },
      "preferredDockedSize": {
        "width": 280,
        "height": 400
      },
      "preferredFloatingSize": {
        "width": 280,
        "height": 400
      },
      "label": {
        "default": "[ss] Panel"
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "./icons/ss_logo.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "darkest",
            "dark",
            "medium",
            "light",
            "lightest",
            "all"
          ]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "./icons/ss_plugin.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium",
        "lightest",
        "light",
        "all"
      ],
      "species": [
        "pluginList"
      ]
    }
  ]
}
